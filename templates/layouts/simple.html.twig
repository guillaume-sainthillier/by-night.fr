{% extends 'base.html.twig' %}

{% block layout %}
    <h1>{% block titre 'Hello' %}</h1>
    <div class="{% block container_class 'container' %}" id="container">
        {% include 'layouts/_flash-messages.html.twig' %}
        <div class="card">
            <div class="card-body">
                <div class="px-md-5">
                    {% block layout_body %}{% block body %}{% endblock %}{% endblock %}
                </div>
            </div>
        </div>
    </div>
{% endblock %}
