{
    "name":  "gsa/bynight",
    "description": "By Night",
    "homepage": "https://by-night.fr",
    "type": "project",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Guillaume Sainthillier",
            "email": "hello@silarhi.fr",
            "homepage": "https://sainthillier.fr",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4 || ^8",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-xmlreader": "*",
        "ext-zip": "*",
        "abraham/twitteroauth": "^2.0",
        "aws/aws-sdk-php": "^3.154",
        "beberlei/doctrineextensions": "^1.2",
        "easycorp/easyadmin-bundle": "^3.1",
        "erusev/parsedown": "^1.7",
        "facebook/graph-sdk": "^5.7",
        "friendsofsymfony/elastica-bundle": "^5.0",
        "friendsofsymfony/http-cache-bundle": "^2.6",
        "google/recaptcha": "^1.2",
        "jms/serializer-bundle": "^3.2",
        "knplabs/knp-paginator-bundle": "^5.2",
        "knpuniversity/oauth2-client-bundle": "^2.1",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-bundle": "^1.2",
        "league/glide-symfony": "^1.0",
        "league/oauth2-facebook": "^2.0",
        "league/oauth2-google": "^4.0",
        "mhujer/breadcrumbs-bundle": "^1.4",
        "neitanod/forceutf8": "^2.0",
        "nyholm/psr7": "^1.4",
        "php-amqplib/rabbitmq-bundle": "^2.2",
        "presta/sitemap-bundle": "^2.1",
        "sensio/framework-extra-bundle": "^6.1",
        "sentry/sentry-symfony": "^4",
        "social-links/social-links": "^1.13",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/asset": "^5.3",
        "symfony/console": "^5.3",
        "symfony/css-selector": "^5.3",
        "symfony/doctrine-bridge": "^5.3",
        "symfony/dom-crawler": "^5.3",
        "symfony/dotenv": "^5.3",
        "symfony/event-dispatcher": "^5.3",
        "symfony/expression-language": "^5.3",
        "symfony/flex": "^1.6",
        "symfony/form": "^5.3",
        "symfony/framework-bundle": "^5.3",
        "symfony/http-client": "^5.3",
        "symfony/http-foundation": "^5.3",
        "symfony/inflector": "^5.3",
        "symfony/intl": "^5.3",
        "symfony/mailer": "^5.3",
        "symfony/mime": "^5.3",
        "symfony/monolog-bridge": "^5.3",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/process": "^5.3",
        "symfony/routing": "^5.3",
        "symfony/security-bundle": "^5.3",
        "symfony/stopwatch": "^5.3",
        "symfony/string": "^5.3",
        "symfony/translation": "^5.3",
        "symfony/twig-bridge": "^5.3",
        "symfony/twig-bundle": "^5.3",
        "symfony/validator": "^5.3",
        "symfony/web-link": "^5.3",
        "symfony/webpack-encore-bundle": "^1.6",
        "symfony/yaml": "^5.3",
        "symfonycasts/reset-password-bundle": "^1.1",
        "symfonycasts/verify-email-bundle": "^1.0",
        "twig/extra-bundle": "^3.0",
        "twig/intl-extra": "^3.0",
        "twistor/flysystem-http": "^0.2.0",
        "vich/uploader-bundle": "^1.9"
    },
    "require-dev": {
        "rector/rector": "^0.11.0",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.11",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "vimeo/psalm": "^4.8"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^5.3"
        }
    }
}
