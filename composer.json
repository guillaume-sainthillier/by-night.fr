{
    "name":  "gsa/bynight",
    "description": "By Night",
    "homepage": "https://by-night.fr",
    "type": "project",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Guillaume Sainthillier",
            "email": "hello@silarhi.fr",
            "homepage": "https://sainthillier.fr",
            "role": "Developer"
        }
    ],
    "autoload": {
        "files": ["lib/functions.php"],
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-xmlreader": "*",
        "ext-zip": "*",
        "abraham/twitteroauth": "^3",
        "aws/aws-sdk-php": "^3.154",
        "beberlei/doctrineextensions": "^1.2",
        "brianmcdo/image-palette": "^2.0",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.11",
        "easycorp/easyadmin-bundle": "^4",
        "erusev/parsedown": "^1.7",
        "friendsofsymfony/elastica-bundle": "^6",
        "friendsofsymfony/http-cache-bundle": "^2.11",
        "google/recaptcha": "^1.2",
        "jms/serializer-bundle": "^4",
        "knplabs/knp-paginator-bundle": "^5.2",
        "knpuniversity/oauth2-client-bundle": "^2.1",
        "league/color-extractor": "^0.3.2",
        "league/flysystem-aws-s3-v3": "^2",
        "league/flysystem-bundle": "^2",
        "league/glide-symfony": "^2",
        "league/oauth2-facebook": "^2.0",
        "league/oauth2-google": "^4.0",
        "mhujer/breadcrumbs-bundle": "^1.4",
        "neitanod/forceutf8": "^2.0",
        "nyholm/psr7": "^1.4",
        "php-amqplib/rabbitmq-bundle": "^2.2",
        "presta/sitemap-bundle": "^3",
        "sensio/framework-extra-bundle": "^6.1",
        "sentry/sentry-symfony": "^4",
        "social-links/social-links": "^1.13",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "5.4.*",
        "symfony/console": "5.4.*",
        "symfony/css-selector": "5.4.*",
        "symfony/doctrine-bridge": "5.4.*",
        "symfony/dom-crawler": "5.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/event-dispatcher": "5.4.*",
        "symfony/expression-language": "5.4.*",
        "symfony/flex": "^2",
        "symfony/form": "5.4.*",
        "symfony/framework-bundle": "5.4.*",
        "symfony/http-client": "5.4.*",
        "symfony/http-foundation": "5.4.*",
        "symfony/inflector": "5.4.*",
        "symfony/intl": "5.4.*",
        "symfony/mailer": "5.4.*",
        "symfony/messenger": "5.4.*",
        "symfony/mime": "5.4.*",
        "symfony/monolog-bridge": "5.4.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/process": "5.4.*",
        "symfony/proxy-manager-bridge": "5.4.*",
        "symfony/routing": "5.4.*",
        "symfony/security-bundle": "5.4.*",
        "symfony/stopwatch": "5.4.*",
        "symfony/string": "5.4.*",
        "symfony/translation": "5.4.*",
        "symfony/twig-bridge": "5.4.*",
        "symfony/twig-bundle": "5.4.*",
        "symfony/validator": "5.4.*",
        "symfony/web-link": "5.4.*",
        "symfony/webpack-encore-bundle": "^1.6",
        "symfony/yaml": "5.4.*",
        "symfonycasts/reset-password-bundle": "^1.1",
        "symfonycasts/verify-email-bundle": "^1.0",
        "twig/extra-bundle": "^3.0",
        "twig/intl-extra": "^3.0",
        "vich/uploader-bundle": "^1.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.12",
        "symfony/browser-kit": "5.4.*",
        "symfony/debug-bundle": "5.4.*",
        "symfony/maker-bundle": "^1.11",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/web-profiler-bundle": "5.4.*",
        "vimeo/psalm": "^4.8"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "platform": {
            "php": "8.0.10"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.4.*"
        }
    }
}
