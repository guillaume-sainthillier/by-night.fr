{
    "name":  "gsa/bynight",
    "description": "By Night",
    "homepage": "https://by-night.fr",
    "type": "project",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Guillaume Sainthillier",
            "email": "hello@silarhi.fr",
            "homepage": "https://sainthillier.fr",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "~8.4.0",
        "ext-amqp": "*",
